{
  "name": "custom-module-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/builder-webpack5": "^6.3.2",
    "@storybook/manager-webpack5": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-loader": "^8.2.2",
    "cpy-cli": "^5.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "json-loader": "^0.5.7",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.69.5",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.43.0"
  },
  "scripts": {
    "start": "yarn sass-dev & yarn storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "yarn clean && tsc && yarn sass-prod && yarn run copy-files && yarn inject-css",
    "clean": "rm -rf build",
    "sass-dev": "sass --watch --update --style=expanded src/scss:_dev_tmp/css",
    "sass-prod": "sass --no-source-map --style=compressed src/scss:_dev_tmp/css",
    "inject-css": "ts-node utils/inject-styles.ts",
    "copy-files": "cpy 'src/schema.json' 'build'"
  }
}
